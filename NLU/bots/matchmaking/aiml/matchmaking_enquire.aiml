<aiml version="1.0.1" encoding="UTF-8">

    <!-- ROOT CASES -->

    <category>
        <pattern>MATCHMAKE ENQUIRE FRIEND *</pattern>
        <template>
            <think>
                <set name="friend"><star index="1"/></set>
                <set name="responder">matchmaking_enquire</set>
            </think>
        </template>
    </category>

